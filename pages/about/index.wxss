/* pages/about/index.wxss */
.about {
  display: flex;
  flex-direction: column;
  padding: 10px 20px 100px 20px;
}
.header {
  font-size: 18px;
  color: #17233d;
  margin-bottom: 10px;
}
.photo-wrapper {
  margin-bottom: 10px;
}
.photo {
  width: 80px;
  height: 80px;
  border-radius: 10px;
}
.info-wrapper {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.info-item {
  margin-bottom: 15px;
  line-height: 22px;
  font-size: 14px;
  color: #888;
}
.key {
  color: #666;
}